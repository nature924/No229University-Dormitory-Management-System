<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.usc.lzh.doms.mapper.MenuMapper">
    <select id="findMenuList" resultType="Menu" parameterType="java.lang.Integer">
        select * from menu
        where utype = #{utype}
    </select>
</mapper>